Algoritmo "Exercício 3"

Var
letra : caractere

Inicio
escreva("Digite F ou M -> ")
leia(letra)

se (letra = "M") ou (letra = "m") entao
   escreva("Sexo Masculino!")
senao
    se(letra = "F") ou (letra = "f") entao
      escreva("Sexo Feminino!")
    senao
        escreva("Sexo inválido")
    fimse
fimse
Fimalgoritmo